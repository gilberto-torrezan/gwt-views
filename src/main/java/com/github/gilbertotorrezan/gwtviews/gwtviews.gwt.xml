<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD GWT Web Toolkit 2.7.0//EN" "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module rename-to='gwtviews'>
	<!-- Module inherits -->
	<inherits name='com.google.gwt.user.User' />
	<inherits name='com.google.gwt.place.Place' />
	<inherits name="com.google.gwt.inject.Inject"/>
	<inherits name="com.arcbees.analytics.Analytics"/>
	
	<generate-with class="com.github.gilbertotorrezan.gwtviews.rebind.NavigationManagerGenerator">
		<when-type-assignable class="com.github.gilbertotorrezan.gwtviews.client.INavigationManager"/>
	</generate-with>
	
	<generate-with class="com.github.gilbertotorrezan.gwtviews.rebind.PresenterGenerator">
        <when-type-assignable
            class="com.github.gilbertotorrezan.gwtviews.client.AutoPresenter" />
    </generate-with>

	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />
	
</module>
